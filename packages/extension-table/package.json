  },
  "devDependencies": {
    "@tiptap/extension-table": "^3.13.0",
    "@tiptap/extension-table-cell": "^3.13.0",
    "@tiptap/extension-table-header": "^3.13.0",
    "@tiptap/extension-table-row": "^3.13.0",
    "@tiptap/react": "^3.13.0",
    "prosemirror-model": "^1.25.4",
    "prosemirror-state": "^1.4.3",
    "@tiptap/core": "^3.13.0",
    "@tiptap/pm": "^3.13.0",
    "tsup": "^8.5.1",
    "typescript": "^5.6.0"
  },
  "peerDependencies": {
    "prosemirror-model": "^1.25.4",
    "prosemirror-state": "^1.4.3",
    "@tiptap/extension-table": "^3.13.0",
