{
  "name": "@easy-editor/context",
  "version": "0.0.1",
  "private": false,
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch --silent --dts",
    "build": "tsup src/index.ts --format esm,cjs --dts --out-dir dist",
    "clear": "rm -rf dist"
  },
  "description": "编辑器的类型声明",
  "devDependencies": {
    "@tiptap/core": "^3.13.0",
    "@tiptap/react": "^3.13.0",
    "tsup": "^8.5.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "typescript": "^5.6.0"
  },
  "peerDependencies": {
    "@tiptap/core": "^3.13.0",
    "@tiptap/react": "^3.13.0",
    "react": ">=18",
    "react-dom": ">=18"
  }
}
