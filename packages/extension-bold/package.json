{
  "name": "@easy-editor/extension-bold",
  "version": "0.0.1",
  "private": false,
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "dev": "tsup src/index.ts --watch --silent --dts",
    "build": "tsup src/index.ts --format esm,cjs --dts --out-dir dist"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.6.0"
  },
  "peerDependencies": {
    "@tiptap/core": "^3.13.0",
    "@tiptap/extension-bold": "^3.13.0"
  },
  "sideEffects": false
}
